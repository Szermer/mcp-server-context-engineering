{
  "name": "mcp-server-context-engineering",
  "version": "1.0.0",
  "description": "MCP server for context engineering operations - exposes skills and artifacts as executable tools",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mcp-server-context-engineering": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "test": "vitest",
